<div id="lecturer" class="container pt-5 my-4">
    <h3>Выборка для проверяющего</h3>
    <h5>Всего работ: {{lec_am}}</h5>
    <table class="table table-hover">
        {% for key in amount_lecturer_keys %}
        <th>{{key}}</th>
        {% endfor %}
        {% for item in amount_lecturer %}
        <tr>
            {% for data in item %}
            <td>{{data}}</td>
            {% endfor %}
        </tr>
        {% else %}
        <tr>
            <td>No items</td>
        </tr>
        {% endfor %}
    </table>
</div>