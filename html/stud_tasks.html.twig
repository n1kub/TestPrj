 <div id="stud" class="container pt-5 my-4">
    <h3>Выборка всех заданий студента</h3>
    <table class="table table-hover">
        {% for key in stud_tasks_keys %}
        <th>{{key}}</th>
        {% endfor %}
        {% for item in stud_tasks %}
        <tr>
            {% for data in item %}
            <td>{{data}}</td>
            {% endfor %}
        </tr>
        {% else %}
        <tr>
            <td>No items</td>
        </tr>
        {% endfor %}
    </table>
</div>