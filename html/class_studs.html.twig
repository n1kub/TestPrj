<div id="class" class="container pt-5 my-4">
    <h3>Выборка студентов для занятия</h3>
    <table class="table table-hover">
        {% for key in class_studs_keys %}
        <th>{{key}}</th>
        {% endfor %}
        {% for item in class_studs %}
        <tr>
            {% for data in item %}
            <td>{{data}}</td>
            {% endfor %}
        </tr>
        {% else %}
        <tr>
            <td>No items</td>
        </tr>
        {% endfor %}
    </table>
</div>